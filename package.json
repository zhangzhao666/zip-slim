  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangzhao666/zip-slim.git"
  },
  "bugs": {
    "url": "https://github.com/zhangzhao666/zip-slim/issues"
  },
  "homepage": "https://github.com/zhangzhao666/zip-slim#readme",
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "globals": "^15.0.0",
    "rollup": "^4.34.8",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}