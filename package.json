{
  "name": "zip-slim",
  "version": "1.0.1",
  "description": "A lightweight ZIP compression and decompression library for modern JavaScript",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./build/zip-slim.js",
  "types": "./@types/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/zip-slim.js",
      "types": "./@types/index.d.ts"
