{
  "name": "zip-slim",
  "version": "1.0.1",
  "description": "A lightweight ZIP compression and decompression library for modern JavaScript",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "test": "vitest run",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "zip",
    "compression",
    "decompression",
    "javascript"
  ],
  "files": [
    "dist"
  ],
